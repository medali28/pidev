{% extends 'baseback.html.twig' %}


{% block body %}

    <div class="col-md-6 offset-md-3">
        {{ form_start(form, {'attr': {'novalidate': 'novalidate', 'enctype': 'multipart/form-data'}}) }}

        <div class="form-group">
            {{ form_label(form.keyword) }}
            {{ form_widget(form.keyword, {'attr': {'class': 'form-control'}}) }}
            {{ form_errors(form.keyword) }}
        </div>

        <div class ="form-group">
            <button class="btn btn-info mt-2 text-white" >Ajouter</button>
        </div>



        {{ form_end(form) }}
    </div>

{% endblock %}
